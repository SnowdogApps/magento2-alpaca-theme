<page
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_layout.xsd"
>
    <container name="root">
        <referenceContainer
            name="footer-container"
            remove="true"
        >
        </referenceContainer>
        <container name="blank-container">
            <block
                class="Magento\Framework\View\Element\Template"
                name="template.test12"
                template="Magento_Theme::blank-notice.phtml"
                after="-"
            >
                <block class="Magento\Cms\Block\Page" name="cms_page_content"/>
            </block>
        </container>
    </container>
</page>
