<script type="text/javascript">
require([
  'focusVisible',
  'lazysizes',
  'picturefill',
  'closestPolyfill'
]);
require([], function() {
  // polyfill for nodeList.forEach method
  NodeList.prototype.forEach = function(callback) {
    for (var i = 0; i < this.length; i++) {
      callback.call(this, this[i], i, this);
    }
  };
});
</script>
